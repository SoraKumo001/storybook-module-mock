{
  "scripts": {
    "test": "yarn storybook:build && npm-run-all -p -r storybook:start storybook:test",
    "storybook": "rimraf node_modules/.cache && storybook dev -p 9001",
    "storybook:build": "rimraf node_modules/.cache && storybook build",
    "storybook:start": "http-server -s -p 9001 storybook-static",
    "storybook:test": "test-storybook --url http://127.0.0.1:9001 --coverage",
    "sf": "scaffold create -t https://github.com/node-libraries/scaffold/tree/master/templates/storybook6",
    "lint": "eslint --fix ./src",
    "lint:fix": "eslint --fix ./src",
    "link": "yarn link storybook-addon-module-mock",
    "unlink": "yarn unlink storybook-addon-module-mock"
  },
  "dependencies": {
    "next": "^13.4.19",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@node-libraries/scaffold": "^1.0.2",
    "@storybook/addon-coverage": "^0.0.9",
    "@storybook/addon-essentials": "^7.3.2",
    "@storybook/addon-interactions": "^7.3.2",
    "@storybook/jest": "^0.2.1",
    "@storybook/nextjs": "^7.3.2",
    "@storybook/test-runner": "^0.13.0",
    "@storybook/testing-library": "^0.2.0",
    "@types/node": "^20.5.1",
    "@types/react": "^18.2.20",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-storybook": "^0.6.13",
    "http-server": "^14.1.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.28",
    "rimraf": "^5.0.1",
    "sass": "^1.66.1",
    "storybook": "^7.3.2",
    "storybook-addon-module-mock": "1.0.15",
    "typescript": "^5.1.6"
  },
