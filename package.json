{
  "scripts": {
    "test": "yarn storybook:build && npm-run-all -p -r storybook:start storybook:test",
    "storybook": "rimraf node_modules/.cache && storybook dev -p 9001",
    "storybook:build": "rimraf node_modules/.cache && storybook build",
    "storybook:start": "http-server -s -p 9001 storybook-static",
    "storybook:test": "test-storybook --url http://127.0.0.1:9001 --coverage",
    "sf": "scaffold create -t https://github.com/node-libraries/scaffold/tree/master/templates/storybook6",
    "lint": "eslint --fix ./src",
    "lint:fix": "eslint --fix ./src",
    "link": "yarn link storybook-addon-module-mock",
    "unlink": "yarn unlink storybook-addon-module-mock"
  },
  "dependencies": {
    "next": "^13.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@node-libraries/scaffold": "^1.0.0",
    "@storybook/addon-coverage": "^0.0.8",
    "@storybook/addon-essentials": "^7.0.2",
    "@storybook/addon-interactions": "^7.0.2",
    "@storybook/jest": "^0.1.0",
    "@storybook/nextjs": "^7.0.2",
    "@storybook/test-runner": "^0.10.0",
    "@storybook/testing-library": "^0.1.0",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.33",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-storybook": "^0.6.11",
    "http-server": "^14.1.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "rimraf": "^4.4.1",
    "sass": "^1.60.0",
    "storybook": "^7.0.2",
    "storybook-addon-module-mock": "1.0.13",
    "typescript": "^5.0.3"
  },
  "license": "MIT",
